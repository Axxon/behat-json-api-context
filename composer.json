{
    "name": "alsciende/behat-json-api-context",
    "description": "Behat context to test your JSON API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alsciende",
            "email": "alsciende@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Alsciende\\Behat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "ext-json": "*",
        "beberlei/assert": "^3.2",
        "behat/behat": "^3.5",
        "justinrainbow/json-schema": "^5.2",
        "symfony/browser-kit": "5.0.*",
        "symfony/http-kernel": "5.0.*",
        "symfony/lock": "5.0.*",
        "symfony/process": "5.0.*",
        "symfony/property-access": "5.0.*",
        "symfony/var-dumper": "5.0.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "0.12.*",
        "phpunit/phpunit": "^7.5"
    }
}
